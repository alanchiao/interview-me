<%# HTML partial for showing test cases associated with a coding problem %>
<div class="panel panel-default" id="<%= @test.id %>-display">
	<div class="panel-heading">
	  <h4 class="panel-title">
	    <a data-toggle="collapse" href="#<%= @test.id %>">
	      <i class="fa-fw fa fa-square"></i>Test <%= @index + 1 %>
	    </a>
	  </h4>
	</div>
	<div id="<%= @test.id %>" class="panel-collapse collapse">
	  <div class="panel-body">
	  	Write your solution and click "Run my code" to see the tests.
	  </div>
	</div>
</div>